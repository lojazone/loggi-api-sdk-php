{
    "name": "lojazone/loggi-api-sdk-php",
    "type": "library",
    "license": "MIT",
    "description": "Library to abstract Loggi APi implementation",
    "authors": [
        {
            "name": "Rafael Pac√≠fico",
            "email": "rafael@webstylus.com.br"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5",
      "ext-json": "*"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1",
        "filp/whoops": "^2.7"
    },
    "autoload": {
        "psr-4": {
